{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "/assets/*",
      "/*.{css,js,png,jpg,gif,jpeg,svg,ico,woff,woff2,ttf,eot,webp,avif}",
      "/images/*",
      "/favicon.ico",
      "/manifest.json",
      "/robots.txt"
    ]
  },
  "routes": [
    {
      "route": "/api/*",
      "allowedRoles": ["anonymous"]
    }
  ],
  "mimeTypes": {
    ".js": "text/javascript",
    ".css": "text/css"
  }
}
